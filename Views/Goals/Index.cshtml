
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Cầu thủ ghi bàn</h2>

<table class="table">
    <tr>
        <th>Tên cầu thủ</th>
        <th>Số bàn ghi</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PlayerName</td>
            <td>@item.NumberOfGoal</td>

        </tr>
    }
</table>